<resources>
    <!-- Device keyhandlers -->
    <java-symbol type="array" name="config_deviceKeyHandlerLibs" />
    <java-symbol type="array" name="config_deviceKeyHandlerClasses" />

    <!-- Post reset runnable for all clients -->
    <java-symbol type="bool" name="config_fingerprintPostResetRunnableForAllClients" />

    <!-- Whether to cleanup fingerprints upon connection to the daemon and when user switches -->
    <java-symbol type="bool" name="config_cleanupUnusedFingerprints" />

    <!-- In-display Fingerprint -->
    <java-symbol type="bool" name="config_supportsInDisplayFingerprint" />

    <!-- Doze for FODScreen off -->
    <java-symbol type="bool" name="config_fodScreenOffDoze" />

    <!-- Vendor signatures -->
    <java-symbol type="array" name="config_vendorPlatformSignatures" />

    <!-- Whether to allow process with media UID to access CameraServiceProxy -->
    <java-symbol type="bool" name="config_allowMediaUidForCameraServiceProxy" />

    <!-- Package Manager -->
    <java-symbol type="array" name="config_deviceDisabledComponents" />
    <java-symbol type="array" name="config_globallyDisabledComponents" />
    <java-symbol type="array" name="config_forceEnabledComponents" />

    <!-- Rotation sensor -->
    <java-symbol type="bool" name="config_useSystemClockforRotationSensor" />

    <!-- Leds -->
    <java-symbol type="bool" name="config_intrusiveBatteryLed" />

    <!-- Whether device has a physical display cutout -->
    <java-symbol type="bool" name="config_physicalDisplayCutout" />

    <!-- Whether device has a big physical display cutout -->
    <java-symbol type="bool" name="config_physicalDisplayCutoutCentered" />

    <!-- Component name of popup camera service -->
    <java-symbol type="string" name="config_popUpCameraServiceComponentName" />

    <!-- Path to fast charging status file to detect whether an oem fast charger is active -->
    <java-symbol type="string" name="config_oemFastChargerStatusPath" />

    <!-- Expected value from fast charging status file -->
    <java-symbol type="string" name="config_oemFastChargerStatusValue" />

    <!-- Pocket bridge -->
    <java-symbol type="string" name="config_pocketBridgeSysfsInpocket" />
    <java-symbol type="bool" name="config_pocketModeSupported" />
  
    <!-- Pocket Service -->
    <java-symbol type="string" name="config_pocketJudgeVendorSensorName" />

    <!-- Power menu reboot -->
    <java-symbol name="reboot_to_bootloader_title" type="string" />
    <java-symbol name="reboot_to_bootloader_message" type="string" />
    <java-symbol name="reboot_to_recovery_title" type="string" />
    <java-symbol name="reboot_to_recovery_message" type="string" />
    <java-symbol name="reboot_system_title" type="string" />
    <java-symbol name="reboot_system_message" type="string" />
    <java-symbol type="array" name="config_rebootActionsList" />
    <java-symbol type="array" name="values_globalActionsList" />

    <!-- Screen recorder framerate limit -->
    <java-symbol type="integer" name="config_screenRecorderMaxFramerate" />
</resources>
